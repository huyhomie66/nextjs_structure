{
  "name": "with-docker",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon server tsc",
    "build": "next build ",
    "start": "NODE_ENV=production node server",
    "build-docker": "docker build -t next-app .",
    "build-docker-multistage": "docker build --target production -t next-app -f ./Dockerfile.multistage .",
    "run-docker": "docker run --rm -it -p 3000:3000 next-app",
    "type-check": "tsc",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "animejs": "^3.2.1",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "dotenv": "8.2.0",
    "next": "latest",
    "next-cookie": "^2.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scroll": "^1.8.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "patch-package": "^6.4.7",
    "babel-plugin-styled-components": "^1.12.0",
    "@zeit/next-css": "^1.0.1",
    "next-compose-plugins": "^2.2.1",
    "javascript-obfuscator": "^2.11.0",
    "next-transpile-modules": "^6.4.0",
    "webpack-obfuscator": "^3.3.0",
    "webpack": "4.44.1",
    "@svgr/webpack": "5.5.0",
    "@types/next-auth": "^3.7.4",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.1",
    "@types/react-scroll": "^1.8.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "4.0",
    "url-loader": "4.1.1"
  },
  "license": "MIT"
}
